        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Este, k'omo chawechaq xatpe taq neri la jaa qik'il, este iin chwaj inyoloow jun kiitz chirij &lt;jun&gt; jun kristyan re jun historia bíblica,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> que iin poq ingan, sí, &lt;d,eh&gt; ingan porque xk'ut jun montón qleen chuwe iin</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> eh ri wunaq ri jb'iij Noé, tb'iij laj tziij qaqaaj Dios, eh que ojer taq tziij kristyan xk'iy jmaakaq wiich uleew,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> chuch Dios, ta' chki' tkojon taq, nijun tjtok Dios, ta' chki' txukar taq chúwch, ta' chki' tjtok taq,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> ta' chki' tran, ta' chki' &lt;t&gt;tkojon taq laq q'aab' Dios, juntiir xtok lo que raj nen tran taq, xk'ex jb'ee'aq,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> jwi'l ri Dios, eh &lt;xpe&gt; xpe k'ax laj ránm, eh xb'ison chirijaq y xpe retzal, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> xan, y xchomrsaj tjk'is jwiich, eh &lt;jja&gt; tjk'is jwiich jmalda'aq,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> juntiir lo que re rechaq tran taq wiich uleew, nijun chik kristyan tran ri utziil wiich uleew, xaq jun jb'iij Noé, pach rixóql y kiib' jk'ajol  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> eh jwi'l li Dios xchomorsaj, eh xyoloow rik'il Noé, xij re lo que, lo que raj tran, raj tjk'is jwiich kristyan, jwi'lVR, jwi'l jmaakaq</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> xk'iy jmaakaq wiich uleew, jwi'l li raj &lt;tj&gt;tjsách bí jwiich wiich uleew, xij re que tran construir jun arca, re tjkol rib' re re pach rixóql</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> kiib' jk'ajol y juntiir eh también eh ka'n wiich uleew, Dios xij re que tjtaq b'ik eeeh</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> ka'n re &lt;tij&gt; tjk'ol xilj man arca y jilon li xan, Noé xkojon chuch Dios, xan lo que re re xan, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> xan lo que re re xij, xan lo que Dios xij re, cuando Dios xij que tjsách jwiich uleew, Noé xpe b'is laj ránm, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> xij re Dios que si looq' tjb'ij re kristyan si raj tjk'ex jb'ee'aq y jilon li tjkolmaj taq &lt;chuch&gt; chuch jaab', y Dios xan decidir taqch jun q'alaj re, tj  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> para que ta' chki' maldad wiich uleew y Noé xyoloow rk'il kristyan, xij rechaq que Dios raj tran</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> tresaj bik juntiir jmaakaq wiich uleew, pero kristyan &lt;ta'&gt; ta' xkojon taq, xtzee'j taq jwiich, ritz'b'ej taq jwiich,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> xan taq jun montón qleen &lt;chuch&gt; chuch Noé, porque ta' tjkoj taq lo que tijb'ij, tnatun taq lékj, y tril taq &lt;que&gt; que &lt;ta'&gt; ta' tk'utun que tpe jaab'. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> Noé xan construir, Noé xan &lt;man&gt;man arca eh sáq'j q'iij tinchomrsaj iin, jwi'l li kristyan &lt;to'oq&gt;, titzee'n taq, &lt;tj&gt; tjretz'b'ej taq jwiich y</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> y &lt;ta',x&gt; ta' xkojon taq, jwi'l li cuand xtaw tiempo, &lt;xx&gt; xq'alji'n lékj suutz' nim, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> na´r que tpe jaab', kristyan &lt;ni&gt; ni aún jilo li xsách eh jpensaraq, ni jilo li xk'ex jb'ee'aq, y xtok taq Dios, sino</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> xan taq lo que re re rajaq, xtzee'j taq jwiich Noé y jfamilia, xpe k'ure' jaab', cada vez xnimar más jwiich y cada vez, xpe más y más,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> y hasta que xpe q'alaj chirijaq, y xaqx eh xkolmaj taq eh k'is ka'n lo que Dios xtaq b'i &lt;re&gt; re Noé y  jfamilia.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> Después de li xq'ax cuarenta aq'ab' y cuarenta q'iij &lt;que&gt; que xwa'x taq l arca, toces </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> tjb'ij &lt;chuch&gt; chuch laj tziij &lt;qa&gt; qaqaaj Dios, que Dios xteb'a arca, porque ri re xtz'apin re desde jun principio, y xteb'a chik, y xel taq;   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE> eh wiich uleew diferente, ri ta' chki' cheka nab'ee, ta' chki' kristyan, ta' chki' jmaakaq, ta' chki' nijun wiich uleew, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE> y lo que wi' lajori, lo que ooj qil lajori, jwi'l li qaqaaj Dios, ri xkan ya'woq, ri xkolow re k'is awaj wi' taq wiich uleew, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> Jwi'l &lt;li&gt; li también que tpe &lt;laj&gt; laj wánm iin, &lt;que&gt; que kristyan jilo li jb'anik cheka ójr, talq'an taq, tjmaj rixóqlaq, tjtij taq awryent </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> ch'o'ji'n taq, eh txukar taq &lt;chuch&gt; chuch eh tyoox y li ta' raj qaqaaj Dios, jlon tjb'ij laj tziij, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE> y tpe &lt;laj&gt; laj wánm que, en jun tiempo ri, Dios ya jiin, jiin tjye' tiempo re wunaq, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE> re kristyan, para que tjk'ex jb'ee'aq, para que tkuxtaj taq Dios, ojxuker chuch, porque ¿nen xya'w qak'aslemal?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31" TIME_SLOT_REF1="ts61" TIME_SLOT_REF2="ts62">
                <ANNOTATION_VALUE> ¿nen más xya'w? si xaq Dios qe xya'w chqe, ta' looq' qak'ex qab'ee, ta' looq' ojb'i xukar chuch jun chik, que ri' ta' tya'w qachóq'b', que ri' ta' tya'w qak'aslemal  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts63" TIME_SLOT_REF2="ts64">
                <ANNOTATION_VALUE> woj jwi'l qaqaaj Dios, tjye' qáw, tjye' teew chqe, tjye' jun montón qleen que ooj ni siquiera xqa ta'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a65" TIME_SLOT_REF1="ts65" TIME_SLOT_REF2="ts66">
                <ANNOTATION_VALUE> Jwi'l qajaw Dios ataq wataq neri, ooj juntiir woj neri, entonces &lt;iin&gt; iin ri tchwajin que kristyan &lt;ti&gt; titran pensar ra historia ri, porque </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a66" TIME_SLOT_REF1="ts67" TIME_SLOT_REF2="ts68">
                <ANNOTATION_VALUE> juntiirVR lo que xq'asaj Noé ójr, jilo li jiin tq'ax lajori, kamnaq por todos lados, de donde sea, &lt;ta'&gt; ta', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a67" TIME_SLOT_REF1="ts69" TIME_SLOT_REF2="ts70">
                <ANNOTATION_VALUE> ta' ni siquiera jun kristyan &lt;que&gt; que tran utziil lajori, juntiir ooj wi' &lt;qa&gt; wi' qamaak chuch Dios, juntiir laj qánm, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a68" TIME_SLOT_REF1="ts71" TIME_SLOT_REF2="ts72">
                <ANNOTATION_VALUE> toces lo que tqa'an lajori, tina' qachomrsaj na bien lo que chqaj, si chqaj utziil o chqaj que ta' tzi k'is qleen, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a69" TIME_SLOT_REF1="ts73" TIME_SLOT_REF2="ts74">
                <ANNOTATION_VALUE> si tqa'an utziil, wi' jun b'ee tzi, y ojb'e rik'il lékj Dios y si tqa'an ta' tzi, también wi' lamas ojk'isb'en, pero ¿nen raj tb'i k'isb'en li</ANNOTATION_VALUE>
