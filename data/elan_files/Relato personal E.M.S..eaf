    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="E.M.S." TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Un buen día para ustedes, mi nombre es Ernestina Méndez, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> me siento muy feliz de que estén aquí en Uspantán, Guatemala.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> Eeeh ahora lo que voy a decir, no es una historia eeeh pues mi finado abuelo,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> nos dejó enseñado como sembrar milpa, frijol, flores, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> pero flores casi no da aquí porque, no es tan buena la tierra, hay muchas piedras, no está bien,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> entonces nosotros pues, estamos aquí sembrando milpa, frijol, eh güicoy, chilacayote</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> y criámos animalitos, perros, cerdos, ovejas </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> y asi, asi es nuestra vida, asi es como estamos aquí en Uspantán pues, que somos parte de Guatemala. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> Y pues mucho más antes yo hacía ollas (de barro),</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> iba a traer arena, tierra, pero no era está tierra, sino que había una tierra aparte que lo secabamos en el sol,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> cuando se secaba, entonces lo poníamos dentro de un baño, de un apaste, entonces le echamos agua dentro, encima pues, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> cuando ya estaba bien, entonces ya estaba suave, entonces hacíamos ollas de barro, para sembrar flores adentro,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE> o sino en las ollas se ponía agua y se ponía flores dentro también, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> y así, de esa manera crecimos eh con mi mamá, hacíamos comales, hacíamos ollas para venderlos,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE> y así fue como nos creció mi mamá, vendiendo en el mercado, nos compraba nuestras cosas a todos. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE> Pues ahora como ellos pues se murieron, ya no hay nada entonces, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE> eh ya es distinto oooh, ya es otro nuestro oficio, ya es una cosa diferente la que hacemos, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE> pues es la milpa la que sembramos, ¡Sí!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE> Tengo ovejas, la majada que producen (excremento) lo echamos de bajo de la milpa, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE> y vamos a comprar abono en la tienda, en el mercado pues, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE> lo echamos y sino echamos el abono de la tienda, no da nada de elotes la milpa, no da nada, ¡así es!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE> entonces de esa manera es nuestra vida, así es como estamos aquí pues. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE> Y hay una máquina para coser ropa, es una herencia que mi abuelo nos dejo,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> entonces como no hay quien se anime a usar la máquina, entonces yo voy a fiar telas,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE> ¡los coso!, hago güipiles, las blusas pues que se usan y los coso, los vendo; </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE> de esa forma vienen nuestros centavos (ingreso económico) solo así. ¡Así es!.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE> Muchas gracias a ustedes. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
