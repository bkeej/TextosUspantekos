    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="M.M.R" TIER_ID="TRANSCRIPCIÓN">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Eh k'amtzawch taq insiwaan-intilmit, ínb'iij María Mulul, eh wi' treinta y cinco injunb', eh inqaaj-inchuuch: Marcos Mulul Méndez,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> y Petrona Reynoso Sajbín. Eh pues iin, iin, win neri, wchooch neri Jacubí. &lt;eh&gt;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> En tinya' jb'ixk chawe taq re jun aq'ab' ri eh que, tinb'ij chre' k'ojol taq wánq, ataq ék'l, que tinya' jun b'íxk chawechaq que -ma sach taq qayólj-,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> porque lajori &lt;tqa&gt; qayólj tjiin tsach, ta' chki' tqa, ta' chki' tqa'an practicar, entoces iin tinya' jun jb'ixk chawe taq, que ma qaschow porque qayólj</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> pok rajwáxk eh lajori aat awetan tqan que ooj ojb'ek, tqa, ojb'e, &lt;tq&gt; qatijoj qiib' eh ari' mas rajwáx qayólj,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> pero qe ooj ya tqasach, ta' chki' tqil jcholaaj juntiir lo que tqa'an, porque qamaam-qatiit' xk'ut taq buen taq no'j chqáwch, per qe ooj lajori   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> eya tqasach jwiich juntiiro ri, ntoces iin tinya' jun jb'ij chawe taq ataq ék'l, b'an taq jun toq'ob' siempre eh atyoloow taq re qayólj, ma sachow taq,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> m kan aye' taq porque ari' li jk'utem qamaam-qatiit', lo que xya', kan xjye' taq chqáwx, kan jk'ute' taq chqáwx re qasiwaan-qatilmit, iin ari' li &lt;tin&gt; tinya' jb'ix chawechaq que qab'ne' practicar.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> Eh lajori chwaj . . . porque wi' juerte qleen re costumbres que ooj tjiin tqasach jwiich, &lt;poremo&gt; eh por ejemplo: re eh q'iij sant tjiin tqasach jwiich,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> que qamaam-qatiit' sí xkam taq, ya xkan qaye' tka kla', kan sach tqan, toces iin tinya' jun jb'ix chawechaq de que mkan qaye', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> porque también re rechaq aunque ya xkam tqan, pero rajwáxk que tqakuxtaj taq eh porque wi' juerte chqe que ya ta' chki' tkuxtaj li,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> qamaam-qatiit' xkam tqan y ta' chki' &lt;tqa&gt; ta chki' tqakuxtaj taq, ntoces iin sí tinya' jb'ix chawechaq que ma schow taq qacostumbre porque ójr qamaam-qatiit' xk'ut taq, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> de que eh cuando tetaw q'iij sant, tkuxtaj taq qawi'l, ojb'e li komsant, &lt;tqa&gt; qak'am b'i ch'iim, qak'am b'i ajan, qak'am b'i múkun,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> y ojb'e chib'aq hasta &lt;tiqa&gt; qaworb'ej taq, ojewi'n chib'aq, pero lajori ya no, ooj tqab'an como que ya fuera chaq jun itz'aab' tqab'an rechaq re re, tkan qat'oje' taq kla', ntoces también ari' li lo que</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> qe maq qasechow pues, porque ari' li lo más importante &lt;que&gt; que wi' lajori, porque ya lajori qas tz'eet ta' chki' qakojeb' &lt;chuch&gt; chuch qleen, yat pur itz'aab' chaq tqab'an re, ntoces iin tinya' jb'ix chawechaq</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> de que qab'ne' practicar de nuevamente. También tinb'ij chawe taq de que también re navidad ya, ya como tesach chqe también, ya tqa'an perder costumbre, que kita' chki' &lt;tqa&gt; tqak'ul jq'iij;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> toces iin tinb'ije chawechaq insiwaan-intilmit que qab'ne' seguir adelante, ma kan qaseche' juntiir lo q qacostumbre tqab'an re juntiro junab',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> tálvez ne' tjta' taq eh wi' naq qejil qatz'aq kita' chki' raj taq que ooj tqa'an practicar juntiro ri', pero ma qaye' qiib', &lt;qa&gt;qak'ise' jwiich lo que qamaam-qatiit' xka jk'ute' taq chqáwch.   </ANNOTATION_VALUE>
