    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="J.A.V.A" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> K'omotzáwch k'ojol wunaq, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> k'omotzáwch q'opoj ánm</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> tinya' &lt;jun&gt; jun ritzil áwchaq lajori wat taq qik'il, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> pues k'omo chawechaq xatpet taq qk'il, tink'omowaj chawechaq. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> Eh chwaj iin &lt;tin&gt; inyoloow jun kiitz ák'laq, nemo qak'iyik ooj ójr rik'i inqaaj </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> com iin ini'n inwunaq tos más xinchaakun rik'i inqaaj, ta' xinchaakun rik'i inmamá, rik'i &lt;in&gt; innaan, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> sino que xinchaakun rik'i inqaaj, xtikik ab'iix, xqatik kinaq'  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> y también &lt;xqa&gt; xqachjaj quleew, ooj xk'utsaj chqáwch cuando ojb'e l chaak</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> que qachjaj na &lt;qa&gt; qáche, cuando qab'ij o tb'ij inqaaj taqasaj jun áchi re</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> tab'an , tab'an, tawesaj &lt;ach&gt; áchi re tab'an &lt;aw,aw&gt; awchooch la', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> tb'ij inqaaj chwe; tatoka' na que ya ri'j chaq iik', ta' taqasaj man áchi cuando ak'al ral iik' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>  porque tran ka'n, tos ta' más naj tch'ij, entons ri li xkan jkute' inqaaj chínwch iin</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> que cuando tinqasaj jun ínchi re tinb'an wichooch la', tinwolb'ej na que tri'job' iik', tri'job' qatiit' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> &lt;jili&gt; jili xk'utsaj chínwch iin. También semill re íxim xkan ya'saj chwe</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> xij inqaaj chwe: tkan inye' chawe, per tachajaj ma sách jwiich y ri li &lt;wi'&gt; wi' na wk'il lajori</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> porque ri li qas qus ttijsaj waa re, sey que, ta' kow sino chúch'j jwaa ttijsaj re </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> para que ta' k'ax chapik, ta' k'ax jkojik &lt;tran&gt; tran qixóql chuch jaa.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> Toces jinoli xij inpapá chuwe cuand xkam, xkan inye' jun k'eer awuleew, per ta' chwaj iin tak'ayaaj </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> sino que tachajaj, porque ri li natb'el wee, cuand iin xinwa'x chíwch uleew, xkan inye' awuleew aat</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> per ta' chwaj iin tat'ook chi jk'ayaj, porque li ta' ingan iin, tche' q'etz inqaaj chuwe cuand xkam. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a40" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> Tos re cuand xkam xkub'ar b'i ránm, que &gt;xin&gt; xinb'ijVR  re: lo que xab'ij chuwe qaaj ri li tinb'an, y ri li b'anal inwi'l lajori, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a41" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> tinchjaj wuleew, tintik jwiich wuleew y tinchajaj &lt;inse&gt; &lt;in&gt; insemilla xka jye' &gt;inpap&gt; inqaaj, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a42" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> tos ri li kub'al ránm re xkam b'ik, que retami'n que qus xkan eel qleen re, cuand &lt;x&gt; xkami re &lt;xx xka&gt; xkan jb'ij: </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> que tawil achuuch, porque achuuch &lt;ta' chki&gt; cuand ta' chki win iin , ta' chki lamas tkub'ar jch'ool achuuch</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE> tos &lt;ak'la la&gt;, &lt;aaa&gt; atiwunaq aat, tos achuuch tkub'ar jch'ool chawij aat, ri li tinb'an re inchuuch lajori,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE> tyuq wile', ta' win rk'il neri la jaa', per siempre tyuq wile' y cuand iin in'eel b'i naj, tkan inb'ij na re inchuuch lamas inb'e'w   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a46" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> porque jinoli xijsaj chuwe ójor taq tziij, ¡veda! cuando nque sey aat chaq nim kristyan, ¡nque sey! siempre tkan ab'ij na lamas atb'e'w, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> m xaq ta at'eel b'i ch jeri', sino que atyuq na chb'ij,  yaq aqaaj-achuuchaq</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a48" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE>  que at'eel b'ik &lt;ch&gt; chi b'anik jun &lt;ra&gt; jun ra la tajkil naj. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a49" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE> K'omo chawechaq, ri li kiib' uxiib' íntz' xinb'ijVR chawechaq, k'omo chawe xaqyuq taq qk'il y xaq qe li xinb'ij.  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="J.A.V.A" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Buen día joven, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> buen día señorita, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> les doy un cordial saludo ahora que están con nosotros, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> pues gracias a ustedes por venir con nosotros, les agradezco a ustedes.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> Eh quiero hablar un poco con ustedes, de cómo fue que nosotros crecimos con mi papá,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> como yo soy hombre entonces trabajé más con mi papá, no trabajé con mi mamá, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> sino que trabajé con mi papá, sembramos milpa, sembramos frijol, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> y también cuidamos nuestro terreno, a nosotros nos enseñaron que cuando vamos al trabajo, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> que teníamos que cuidar nuestros árboles, cuando decíamos o decía mi papá botá un tu árbol para </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> que hagas, que hagas, para que saqués tu madera, para que hagas tu casa con eso,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> me decía mi papá; tenes que buscar que ya este vieja la luna (luna llena), no botés tu árbol cuando sea nueva luna, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE> porque se va a pudrir, entonces no va a aguantar mucho tiempo, entonces eso fue lo que me dejó enseñado mi papá a mí, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE> que cuando bote mi árbol para hacer mi casa, tengo que esperar que envejezca la luna (luna llena), que envejezca nuestra abuela (luna)</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> así. así me enseñaron a mí. Tambíen semillas de maíz me dejaron a mí,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE> mi papá me dijo: te lo voy a dejar, pero lo cuidas no los vayas a perder y eso es lo que aún tengo ahora, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE> porque es bueno para comer en tortillas, osea que, no está duro sino que suaves las tortillas para comer,    </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE> para que no sea dificil agarrarlo (tortear), que no le cueste ponerlo (como nixtamal) a nuestras esposas en la casa. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE> Entonces así me dijo mi papá cuando murió; te dejé un pedazo de terreno, pero yo no quiero que lo vendas, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE> sino que lo cuides, porque con eso me vas a recordar, que cuando yo estuve en la faz de la tierra, te dejé tu terreno,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE> pero yo no quiero que lo empeces a vender, porque eso no me gusta a mi, me decía mi finado papá cuando murió. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE> Entonces cuando murió quedó confiado, que le dije: lo que me dijiste papá eso es lo que voy a hacer, y eso es lo que estoy haciendo ahora, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a82" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE> cuido mi terreno, siembró en mi terreno y cuido mi semilla que  me dejó mi papá, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
