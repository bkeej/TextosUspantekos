                <ANNOTATION_VALUE> man ka'n li, jun nimlaj ka'ni'n y re re lo que xan &lt;x&gt; xk'ol rib', xjtitz'e' rib' li chrij nik'aj chee',  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> xelch, &lt;al&gt; al ratito xelch &lt;man man&gt; man ka'n xqax chi jnáqaj y tjb'ij re re: -injtija' man k'an-, porque jun nim jun ka'n, entonces   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> xpe re re tjb'ij: -ajori ta' chki' inkw'inik, ajori inkan neri, &lt;porq&gt;man ka'n ri inajtija' b'ik,  ta' chki' inkwínik- </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> y re re ta' chki' tkwin tb'énik, ni ta' chki' looq' ttzaq rqan, xkan kla' y xpet k'itzan inchuuch tjb'ij re chqe: </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> eh -rájw, rájw aq'aab' o rájw jaa'- ta' tinch'oob' nen xinta'w, pero &lt;xin&gt;xinwila' jun nim ka'n,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> y xqil, xwila'n xq'ax &lt;chi&gt;chin náqaj jun nim ka'n, tinwil jrilik pur jun nimlaj tz'ii', pero tz'ii' ta'n, sino que jun jun ka'n eh   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> eh tálvez &lt;ta' tin&gt;, &lt;ta' til&gt;, ta' ilan inwi'l, tinwilin &lt;jun, jun&gt; jun re k'achee'lajin, per ta' tinch'ob' jwiich man ka'n tjb'ij ri chqe ooj;     </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> toces tqab'ij ooj re ¿pero xawila' nemo man ka'n? si xwila'n, y li kaxlan tziij xb'ij &lt;maq&gt; maq panteras verda, li kaxlan tziij,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> y laj qatziij jun nimlaj ka'n re k'achee'laj, y tjb'ij re re &lt;n&gt; -ta' tinch'ob' jwiich man ka'n-, pero man ka'n li xq'ax chin náqaj,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> per iin ta' chki' xinkwin xintzaq wqan, xinkan kla', y ¿ta' nen xan chawe? no, xq'ax b'i chínwch, xq'ax b'ik, cuand xq'ax b'ik iin ta' chki' xinkwin xinb'énik, xinkan kla'     </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> y xwila'n cuand xq'ax b'ik, xq'ax b'i chínwch ri jun nim ka'n, xiqej chuch &lt;jun&gt; jun chee' reeem re ooj, wi' jun chee' nim chee' la' re ooj,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> xi' taw man ka'n chuch y wi' jun re luz que tná'rik, kojol kla', y tjb'ij re re man ka'n ri' ta' chki' man ka'n,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> sino que man ka'n xan b'ik, xan b'ik jun ka'n chíwch y xi'qej chuch man nim ooj, xwá'rik y ma ri' ta' chki' man ka'n sino que jun wunaq chaq xwa'r b'ik,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a50" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>  y xril re re &lt;ri' man&gt; ri' man ka'n y tjb'ij jun wunaq que &lt;x&gt; qesel b'ik, tjb'ij chwe, &lt;tj&gt; xijb'ij chwe ¿nen xata'w? ta' chki' inch'áwik   </ANNOTATION_VALUE>
