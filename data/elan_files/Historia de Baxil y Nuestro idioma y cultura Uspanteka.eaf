    <TIER ANNOTATOR=" Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="J. Ch" TIER_ID="Descripción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a150" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> B'a Xiil: sobre la silla. Topónimo Uspanteko del Caserío Baxil. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a151" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> Xiil Xill: sonido que produce un tipo de pierdra, que sirvió como topónimo del Caserío Baxil. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a152" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> Xiil o Teem en idioma Uspnateko son palabras sinóminas que significan silla.   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a153" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> Teem o Xiil significan Silla. Son sinomimos y son una variante del Uspanteko.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a154" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> Kurs= cruz. Referencia de un lugar donde se entrelazan dos carreteras formando una cruz.		Chii' puerta: a la orilla de la puerta. Referencia antigua de lo que hoy es Baxil.   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a155" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE> Ch'ola'= Cholá. Aldea de Uspantán que colinda con Caserío Baxil. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a156" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> Ajkot, Vicente y Chitop fueron las primeras familias Uspantekas en residir en Baxil. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a157" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE> Chamac y Cholá son comunidades de Uspantán, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a158" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE> Canaques y Caracol son comunidades de Uspantán.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a159" ANNOTATION_REF="a47">
                <ANNOTATION_VALUE> Chii' puerta= en la orilla de la puerta, B'a xiil= sobre la silla </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a160" ANNOTATION_REF="a75">
                <ANNOTATION_VALUE> Aj Tz'unun Kaab' y  Aj Tinmit son palabras sinónimas que significan Uspanteko, refiriéndose al gentilicio del municipio de Uspantán.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a161" ANNOTATION_REF="a77">
                <ANNOTATION_VALUE> Las palabras Qaaj y Chuuch son palabras homónimas, ya que Qaaj puede significar papá o señor; mientras que Chuuch puede significar mamá o señora.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
