    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="M.A.A" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Tos eh &lt;la&gt; ojor taq tziij cuand xink'iych rik'i inqaaj rik'i inchuuch, aaah que pena  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> muy educado ooj; ojr taq tziij mas ooj respetoso, ojor taq tziij mas alegre xojk'iych ojor taq tziij  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> ¿per lajori? &lt;ojr&gt; ojor taq tziij, per we iin tovía jkanali jch'ab'al inqaaj-inchuuch laj qalugar lajori, kanaliin. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> E tqab'ij ójor cuando ojtaqsaaj  tra maq ri, tqakoj tzii b'a q'aaq', tb'iqesaaj íxim l b'o'j, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> tin jxujb' ojche' re qáq'b'VR, tb'i qame'ch qixiim l b'o'j, tqaye' xilj ra qato'l, &lt;qilemp&gt;  tqat'oje' b'a q'aaq' re tzii, porque tik'uxánik</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> tk'uxán jwiich tzii, tk'uxán íxim mmm y solo jinol cosas l ojor taq tziij, re jun cualquier cosa tin quxlub'ij qáq'b' tiqtzaq che;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> tinb' quxlub'ij qáq'b', jinon konon tb'ijsaaj chqe wi' qanaan-qataat ojor taq tziij, qámam qátit', ¡ji'! </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> y ant tb'ijsaaj chqe tawixlub'ij áq'b'aq con eso tk'uxán qleen, takojt retal ¡eee! ojche', tqakoj retal cosas, ¡ji'n!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> tqakoj retal cosas ojor taq tziij, ojor taq tziij cuando xojk'iych ooj, ya cerca de ochenti'añ, wiin lajori pumundo&lt;mal&gt; k'omo re qaqaaj Dios. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> Eh xink'iych rik'i inqaaj-inchuuch, ta'ch, xi' chaq we iin inyo'lkiin, juntir xi' tqan, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> tir xi' tqan, iin chaq yo'lkiin &lt;kanl&gt;kanalkiin lajori y laj lugar inqaaj, laj lugar inchuuch, kanalkiinw</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> pero ¿por qué? per com xinb'na bien rechaq ojor taq tziij, &lt;xin,xin&gt;xinb'na humillar wib' ojor taq tziij, ¡ji'n! </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>com qe ooj ojor taq tziij pur ooj aj tinmit, ooj aj tinmiti'n ooj pur aj Uspanteko, pur ooj aj Uspanteko.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> Por es tamb'ij iin que ojor taq tziij más alegre xojk'iych, porque ojb'e laj sii', tb'imech sii', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> tb'ime'ch sii' jli rij kalwa'r ojche', jela tiqame'ch sii' re tre qaq'aaq',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> tb'i qeqajch sii', qeqajchim jun qakolob' ojpétk </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> ah ójr taq tziij ta' pen ri,  xojyuqin naan, xojyuqin taat;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> ¡e! miij, t'oj aysi'b'aq kla' walk'uwal, kat'uxlan taq, kat'uxlan taq miij porque ya xatkos tqach,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>tiche' inqaaj chqe, ji'n taat xojkosi'n, per ojnb'ech jkamuul taat porque wi' sii';</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> wi' tra xik'a'y xiqile' taat, chaq tyuq qaye' jun viaj ri, ojnb'e chik</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> ah ta' chki atb'e taq miij porque penti nin nin tb'i ate' taq; no ta'n taata, Dios primero taata  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> y ojb'i'n, ojb'i chki juntir, b'i qame'ch jun b'oraaj qaxik'a'y chik, naqaj taata, naqaj rij Calvario tiqame'ch b'a, que alegre, que pena  ojor taq tziij </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> com teel lugar lamas tb'i qame'wich, com teeli mundo lamas tiqame', per lajori ya no;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a46" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> jo ta' chki tiya'saj lugar chqe lamas tb'i tqame'ch qási' re ojor taq tziij, rin chqe b'ay ne ojor taq tziij cuand xojk'iych;</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE> cuand xojk'iych rk'i qaqaaj-chuuch, xojk'ich ch wunaq, ch tra anm xqak'ama b'a qib' xojb'i'n</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a48" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE> va, xojyuqin xaq qak'ama b'a qapichik'a, qama'ch b'i qapich, xqama' b'i re qaq'iib', y xojb'i chi' jaa', xiqame'ch jaa' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a49" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> va y list chaq jb'o'j inmamá, xqanojsaj qab'o'j re jaa', com l pozo tyuq qame'ch jaa' kon xqanojsaj jb'o'j inmamá re jaa', ah ¡ya xnoji qab'o'j naana!</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a50" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> tos kojta taq tuuj, b'i qakoje' re qatuuj, b'i qamese' pam tuuj, tqak'am b'i qamesb'VR;    tqak'am b'i qamesb'VR, tiqames pam tuuj,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a51" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE> va y tos&lt;eee&gt; ¿ya xnoji b'o'j re jaa'? ya ojche' re inmamá, va tos ojlooq' tqakoj q'aaq'; tiqame' chaj   xqakoja' chaj l tuuj, qaye' tzii chib' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a52" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE> va y cuand tiqej tuuj, xiqame' sii' chpam tuuj, ¡ya xqeji tuuj li!, ya xqeji tuuj naan-taat, ya xqeji atuuj, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a61" TIME_SLOT_REF1="ts61" TIME_SLOT_REF2="ts62">
                <ANNOTATION_VALUE> ¡eh miij ooj chaq b'ek! ya xqatz'apij chii' tuuj; la' jun ra qaq'u'niik xqatz'apij chii' tuuj, ojor taq tziij jinoli tqab'an mmmm jinoli tqab'an ojor taq tziij.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a62" TIME_SLOT_REF1="ts63" TIME_SLOT_REF2="ts64">
                <ANNOTATION_VALUE> Más alegre ojor taq tziij tanwilo, per lajori ta' chki ojkwiin rik'i qach'ab'al; per com ya xqab'ana rovuelta, ¡ya! </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a63" TIME_SLOT_REF1="ts65" TIME_SLOT_REF2="ts66">
                <ANNOTATION_VALUE>  per en cambio we iin nunc tsach chwe iin, nad tsach inch'ab'al chwe iin, nunc tsach inch'ab'al; kanalkiin ójr taq tziij cuand xink'iych rik'i inqaaj rik'i inchuuch, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a64" TIME_SLOT_REF1="ts67" TIME_SLOT_REF2="ts68">
                <ANNOTATION_VALUE> ila' puch inch'ab'al lajori, ajwichin, ajwichin inch'ab'al lajori, wiich wi' inch'ab'al lajori, pues jinonaq chqab'an ojor taq tziij, más ojtzii xojk'iych ojor taq tziij, ta' pena</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a65" TIME_SLOT_REF1="ts69" TIME_SLOT_REF2="ts70">
                <ANNOTATION_VALUE> ojor taq tziij xojk'iych, ojor taq tziij, qaaj hay Dios takuy taq qamaak k'omo chawechaq inyuq asolaj taq, nimlaaj k'omo,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a66" TIME_SLOT_REF1="ts71" TIME_SLOT_REF2="ts72">
                <ANNOTATION_VALUE> mu's k'omo chawe mu's, k'omo chawe noya inyuq asolaj, inyuq asolaj taq; k'omo chawechaq. K'omo chawe qaaj. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="M.A.A" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Entonces eh mucho tiempo atrás cuando crecí con mi papá, con mi mamá, aaah que pena</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> muy educados nosotros antes éramos muy respetuosos, antes era más alegre cuando crecimos antes,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> ¿pero ahorita? antes, pero yo todavía no he olvidado el idioma de mis padres en nuetro lugar (vivienda) ahorita, aún no lo he olvidado.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> Esta bien decíamos antes cuando nos mandaban; pongan eso, poníamos nixtamal sobre el fuego, sacábamos maíz de la olla,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> y teníamos que hacer jxujb' (soplido sagrada para la abundancia) a nuestras manos, para ir a traer nuestro maíz de la olla, para echarlo entre nuestro recipiente, y poner sobre el fuego el nixtamal, porque solo así abundaba, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> abundaba el nixtamal, abundaba el maíz mmm y solo así eran las cosas antes, para cualquier cosa todavía teníamos que soplar nuestras manos para luego hacerlo;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
