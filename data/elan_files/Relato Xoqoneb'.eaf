        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> K'amtzáwch ataq qawáxk, k'om jun rtziil áwchaq, re loq'laj q'iij ri inyoloow chrij ajxojooj. Ójr taq tziij inmaam tjyol taq chqe</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> eeeh wi' jun wnaq eh jgan tixojówk, re eh wnaq li kita' &lt;j-'eh&gt;jpwaq re tjloq' jun rtz'yaq, ¿nen xan chki' jun q'iij jun wnaq li? xk'om b'i jklob'</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> xk'om b'i qleen re, xe' l sii', eh wnaq li xrne' jsii' eh Xoqoneb' tb'ijsaj re loq'laj wiitz, kla' li re re tb'ison,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>  sub'laj tb'ison kita' jpwaq t-'e re xojooj, li kaxlan tb'ij taq re: baile mexicano, ya sea de Conquista, eh loq'laj wnaq li eh   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>  tb'ison kita' kare' nemo tjloq' jpwaq, xo'l maq, maq nimaq taq ab'aj wi', wi' l wiitz Xoqoneb', xeelch jun ra &lt;k'xo&gt;, jun ra saq k'xol,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> tb'ij re ¿nen chak atb'isónk? wunaq ri xij re: inb'isónk, kita' inpwaq, kita' inchaak, &lt;j&gt;iin juert, sub'laj t-'eel wánm in'eel re jun xjooj, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> loq'laj a saq k'xol li xij re: wane' chawaj at-'eel l xojooj, chwaj inyoloow ák'l, xye' ni'j eh, ni'j taq ra qleen re y xij re: ¿nen rechaq </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> chawaj tacha'w? xpe wunaq li xchá'wk ri jun ra wiitz y xij re: ¿tzi' li!, oj'okoq neri il  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> sub' ra saq k'xol ta' ka'x rokik xan b'i la' ra wiitz li, xook b'ik y wnaq &lt;x&gt;xterenb' chrij, xook taq loq'laj wiitz li, ri' chaq tril wnaq eh  eh xpe ra saq k'xol, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> xk'ut ni'j atz'yaq, sub'laj nimaq taq atz'yaq, tjululúk, tijululúnk, y wunaq xij re: wane' aat chawaj takoj man atz'yaq li, koja' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> koja' y at-'eel l xjooj; ruk'u li tinb'ij jun qleen chawe, kita' chwaji'n tayol, wane'n ta'w chawe &lt;lama&gt; &lt;loq', x&gt;  ¿lamas xame'w loq'laj atz'yaq? ta' tab'ij nen xyo'w chawe. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> Jinon li loq'laj wnaq sub'laj content xqejch, xtawk k'i jpach, xaq re re qe k'ak' rtz'yaq y xta' taq re ¿lamas xame'w apwaq xaloq' man k'ak' laj atz'yaq li?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>  eh ¿nen chki' xij aj wunaq? re xij: eh ji' xinme' jli, xaq jnon qe li xij re y rechaq tjta' taq: ¿lamas kixjme' loq'laj atz'yaq?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> y jinon li, com nimq'iij wi' taq, loq'laj nimq'iij li xye'saj awryent y rechaq eh qus rik'aj taq xq'ab'érk eh wnaq li, xye' taq más awryent re</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> y xq'ab'rsaj taq, jin tjta' taq re y loq'laj wnaq ta' tjb'ij lamas xjkame' atz'yaq, xq'ab'rsaj taq, xq'ab'rsaj taq, hasta que xkwin taq chi jresaj  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> nen re o lamas &lt;k'&gt;xkame' atz'yaq y loq'laj wunaq li laj q'ab'erik xyólk lamas xjme'w loq'laj atz'yaq li, xij rechaq: iin xink'ul jun ra &lt;n&gt;saq k'xol, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> xinajkom b'i la' wiitz re Xoqoneb', kla' li xye'ch maq nimaq taq atz'yaq chwe xinkoj, jwi'l k'u li &lt;xin&gt;xin'eel ák'laq l xjooj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> Y jnon k'u li xpe rtzal ra saq k'xol, le teew kyaq' xpe rtzal, xmaj juntiir qleen re y nemo jb'anik wnaq xtaw chk'amik sii', jino jb'anik xkajye' chik, kla' qe taw intziij. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> Jinon chik, wi' k'ii chik wiich taq ra saq k'xol, wi' chi na jun chik xan ójr, jinon li tyol taq, eh wi' chik jun ra saq k'xol xk'ul kiib' wunaq, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> xta' rechaq eh ¿las atb'e'w taq? loq'laj wnaq li ta', ta' jchaakaq, kita' nen tjye' rech rch'ixiikaq re tijwaa, ta'j, ta' jun qleen tye' rechaq taq ra rlk'uwal</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
