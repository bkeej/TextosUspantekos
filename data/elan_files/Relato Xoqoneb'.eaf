        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>  xpe loq'laj saq k'xol li chrij calvario, che' taq re, kla' xta'w rib'aq, y xij re: ¿nen chki' chawaj taq ataq lajori? </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> ooj chqaj qachaak che' loq'laj taq wnaq re ra saq k'xol, xpe loq'laj saq k'xol li xye' jun mulaj de llaves rechaq, xij rechaq:</ANNOTATION_VALUE>
