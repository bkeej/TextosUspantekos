    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="M.L.A.L" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Y nimo inq'uun laq tinmit, &lt;t&gt;xka'el tqa rk'an taq, que rk'an taq tiin tran taq vivir &lt;ch&gt; rb'il rib' taq, ta' jnegos taq, tlej taq waa,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> y nimo tinb'ij que, wi' jun inpena awri, que &lt;wi', jun&gt;  wi' jun wich'ixiik que k'ax wi'w,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> y tinta' re qaqaaj Dios que Dios &lt;tetzib'&gt; tetzib'an re, que Dios primero que tran solvej jyaaj porque</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> eh que tinta' iin tb'ij que k'axVR wi'wVR; nemo tinb'ij re íwr que kuxtaj Dios,  porque xaq Dios qe tan grande, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> wi'j obr, wi' raj jmilagro &lt;tj&gt; tzi laj vid wólb', porque k'ax qe wi'w, nemo tinb'ij iin qaaj-chuuch y xaq li inyólj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="M.L.A.L" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a11" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Y como mis hermanas en el pueblo, se quedaron solas, que solas están viviendo entre ellas, no tienen negocios, solo tortean,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a12" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> y como digo que, tengo una pena ahora, porque tengo un familiar que está enfermo, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a13" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> y le pido a nuestro señor Dios , que lo sane, que Dios primero pueda resolver su enfermedad porque, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a14" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> eh porque yo escucho que dice que esta enfermol; como le dije ayer que se acuerde de Dios, porque solo Dios es tan grande,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a15" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> que tiene obras, que tenga un milagro en la vida de mi suegra, porque solo enferma se está, asi digo yo señores y solo esas son mis palabras.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
