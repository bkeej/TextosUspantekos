        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Eh tink'amwaj cháwchaq xojach'ab'ej taq re ojwa'x jun kiitz ák'laq neri, ri' chqaj &lt;tqa&gt;tiqaye' jb'ij chawechaq nemo </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> qleen wa'sélik y tá'lik laj qatilmit. Chipka' ooj eh ooj elexiik ooj uxiib' eh woj neri lajori y &lt;xi&gt; ya wetz ya xi'j kiib'-uxiib' jtziij nen xta' ór taq tziij y </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> jlon wanaab' nemo xk'iych, nemo xk'utsaj chaak chuch la jaa, y xuk'u iin ta' chki' chwaj &lt;tin&gt; tinya' chik b'ij eh yaaa rechaq xij tqa k'ii qleen naq q'aselkoj y wa'selkoj chuch, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> lajori ri' kya' chqaj tqa, chwaj nikare' tinb'ij chik pues iin wi' chki' que qleen &lt;tqa&gt;wa'sélik y ta'sel laj qatilmit y y wi' vuelt   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>naq sach chqe nemo jb'ij jun qleen, nemo jb'anik qleen eeeh ooj aj Tilmit &lt;tj,j,j&gt; aj Tz'unun Kaab'  pues eh también  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> pur ooj ajchaak cheka xi'j taq qech'elxiik miir, qagani' ojchakúnk, qagan tqil nemo tqach'ek kiib'-uxiib' qleen qe, eh ya también qagani' oj-'itz'ánik, &lt;q&gt;qagani' etz'aab', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> qagani' ojb'e li kut, qagan ojb'e l jaa' l kar&lt;rrr&gt;, chapik kar&lt;rr&gt; l jaa'  eeeh ri' k'u li qleen re tqa, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> re &lt;tqa&gt; ¿nemo jb'ij? tqasach juntiir, jun kiitz &lt;qa&gt;qapena la jaa verdad, oj-'eel b'ik, qatoke' juno qleen qe, tqab'en naq laq wron, tsach jun kiitz &lt;qa&gt;qapen wi' la, wi' laj qchooch. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> Inqaaj eh jun wunaq que más jgana tb'e l kut y xojtijoj ooj pach wetz también ojb'e l kut,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> ojb'e l kut ma q'iij, q'iij ta'n, &lt;ni&gt;, ni chi domingo tane' ¡no! ojb'e l kut re qe lemaq taq q'iij ojcha', tqatok lemaq taq q'iij re las wi'w jq'iij la juno tyoox chipk Simyeel;  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> inqaaj ójr Simyeel ojb'e l kut chi jtokik kyej, chi jtokik eh jalaw yyy xintz'onaj re inqaaj: ¿nen k'u mod li qaaj agan inche' re atb'e l kut? ¿nen k'u mod? ¿nen xk'utuw cháwch?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> aaah wi' jun qiqan, qikan tcha', más jgan tb'e l kut ójr, qikan Liix, eh re más jgan tkutúnik y rrugur ri' li xwesájk tcha', que tjb'ij k'etzan amaam chwe ójr re</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> jun galan k'ojol wánaq wkan Liix, rik'an tb'e l k'ichee'laj, tb'e l kut, tb'e neri Q'eqaltum, Xe' Ch'iyu't, kla' galan taq</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> k'ichee'laj ójr taq tziij, ta' ttaw kristyan kla', kristyan naq taw taq eh Ch'ola', chi jchí Ch'ola', ta' t-'ook kristyan kla' ójr, wi' koj, wi' balom,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> wi' lemaq taq ka'n, ruk' la' wkan ta' tzaq jch'ool, tb'ek rk'an, tjk'om b'i jch'iich', xaq re wi' jch'iich' laq tilmit,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> y tjk'om b'i jch'iich' y tb'ek, y ya cuand tiq'ájch, k'amalch jti'niik jwi'l, wi' vuelt tjkamsajch jun jkyej, tjk'ómch kyej y tjb'ij eh tzane maam tyoloow re intaat.  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>Awikan li eh jor qus jno'j, cuand tyu'k' ya, xojsik'iji' juntiirVR y sik'ijij aj kristyan aj tilmit nen jgan tijtij ti'niik y tqab'ol ti'niik, </ANNOTATION_VALUE>
