        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Buen día me llamo Elssy Méndez, soy de aquí de Jacubí (aldea de Uspantán)</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> yo trabajo en la educación, allá en un lugar llamado Ojo de Agua (aldea de Uspantán),  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> allá en Ojo de Agua nuestra comunidad, allí todas las personas no hablan en nuestro idioma, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> allá hablan español, yo cuando llegué allá eeeh me sentí muy feliz que las personas me hayan recibido..  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> Eeeh hay niños, allí los niños no hablan en nuestro idioma, allí todas las personas son ladinas;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> se dan allá en nuestra comunidad, naranjas, limones y en todas las casas hay un árbol de jocote,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> allí eeeh en el mes de octubre venden jocotes, los venden, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a9">
