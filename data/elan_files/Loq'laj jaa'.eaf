    <TIER ANNOTATOR="Tomás Alberto Méndez López" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="J.CH.T" TIER_ID="Descripción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a157" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> En Uspantán actualmente existe una tala inmoderada de árboles, mucha producción de madera, leña y de carbón, pero no existe conciencia para sembrar más árboles.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a158" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>El relatador vive en el Caserío Baxil, a la par de ellos se encuentra la Aldea Cholá, que cuenta con nacimientos de agua, pero que también se esta acabando.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
