    <TIER ANNOTATOR="Tomás Alberto Méndez López" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="M.D" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE> Saqari'n wxtub'-wech'elxiik tanya' jun rtziil áwchaq lajb'iij qajawal Dios, tzi qáwch xojsaqarsánik, ta' nen xqata'w, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> qaq'axaj chi jun aq'ab', k'mo re nimlaj Dios, tanya' jun rtziil áwchaq, k'omo; qab'i b'i kiib'-uxiib' qatziijl re qaswaan, re qatilmit. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> Ojer taq tziij cuad xinaq'ánch iin niri, we'in ini', &lt;wi'wi',w&gt;wi' wchooch chi calvario rk'i inqaaj-inchuuch rk'i Matías Méndez, rki' Juana Damián,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> kla' wójw ooj chi calvario, kla' wi' rchooch inchuuch-inqaaj y &lt;ximp&gt;xink'b'ik'amsajoch jwi'l jun tzik' pobre persona wájw wchijil xinjb'ijme'ch,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE> xqab'an qacasamiento, xnori' xinb'an conocer Jacubí xinaq'ánchVR, cuando xinyu' iin niri loq'laj b'ee jun q'aab' b'ee qe, ta' carretera, kita' nada, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> peroooo usiil k'is qixtub'-qch'elxiik xu' lajb'aa'aq xwanaqersaj taq jun escuele, xwanaqrsaj taq lamas tya'sajw clase re k'is ék'lVR,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> ari' li xan taq. Arruk' re' xnimar Jacubí, xansaj jwa'x jb'ee porque Jacubí ri ójr nim laj jaa' iri wi', "man jaa' kub'i'", </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> nimlaj jaa' y ii tb'énikVR, pero como xchaqájkVR ta' chki' jaa', wi' nkali' nen xan y norque ójr taq tziij man jaa' li con gan tapanol re, ójr taq tziij pok juert, pok nim,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> per como wi' jfin, juntiir xk'ískVR, keta' chki'n, b'ay k'omo re chawechaq &lt;oj&gt;aj'ab'an taq tomar, tqab'an, qab'ij kiib'-uxub' qatziij re qaswaan-qatilmit. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> Ójr taq tziij ri cuand xink'íychin, mercad li ójr taq tziij k'ii mejb'a, ri' wi' mercado l conventa,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> k'ii mejb'a qacocin li ójr, ooj ajcocin ooj li ójr, &lt;aj&gt; iin nuuch' na, wi' chaq qacocina re rk'i intiit' Matea Cabrera,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE> y &lt;k&gt;xwila' li xeelch mercado lo conventVR, ari' sacerdote padre Gerardo xesánch man mercado li le conventVR, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> xq'axajchl la' man aak' mercado lojori. Ri li qaswaan-qatilmit qaya' cuent re nemod &lt;xch&gt;xanch qatilmit ójr taq tziij, arri' li, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> y xinyu' chki' neri chik, xinwila'n también nemonaq xansaj jwa'x b'ee, nemo xansaj jwa'x carretere, carretere ri tb'e Chejam, keta' carretere re, jun ra q'aab' b'ee qe b'esálikVR,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> pero &lt;yuj&gt;siil xb'it rib qéjel qatzáqte', xan taq jwa'x man carretera tb'e Chejam, xnimarkVR, xan taq jwa'x jun b'ee lamas taq'an jb'aaq loom B'aa Kub'i'</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> también xnimarkVR, per arri' k'u li rajox que tqaya' cuent re juntiir, lo que nemod xan&lt;sus&gt; suceder laj &lt;qa&gt;laj qacomunidad,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> arri' li xan, b'ay we'in gracias a Dios durante los ochenta siete años injúnb' lojori, &lt;xi&gt; ajwichVR win tjiin tab'an innegocio, xinmájch innegocio a los  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> diecisiete, dieciocho añ xinmaj innegocio y ajwi ni xil lajori; k'omo re nimlaj Dios porque ri' re li ya'onaq qachoq'aab' wxtub'-wech'elxiik tawetamaj taq.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> Ari' li qab'anik laj neri laj qaswaan, laj qatilmit, ra qalugar lamas wójw, b'ay k'omo chawechaq ojb'ansaj tomar en cuenta, ooj tiqatyoxij</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> laj q'aab' qijl qatzaq, qextub'-qech'elxiik, ari' li qab'an qanegocio xink'ut chuch k'is inrab'iin, k'is walk'uwal, xinchap taq porque ta' chwaj . . .   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a40" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> ¿nen tach'ekb'ej avidaq algún día? xinche' rechaq cuand xk'iy taqch wk'lal, joob' taq chi ánmVR y kiib' &lt;w&gt;, kiib' wk'lal wunaq lunica y juntiir wk'lal </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a41" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> &lt;siete&gt; siete rb'ilaq maq ánmaq juntiir ¿nen tank'ut chuchaq? ¡mo que tkan ink'ute' mta' tzi chuchaq! ¡mo tkan ink'ute' alaq' chuchaq! ¡nooo! xink'ut jnegoci'aq,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a42" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> xink'ut jb'anik, ch'ekik jvidaq, ch'ek, tch'ek ski' jvidaq, xink'ut jb'anik jnegoci'aq chuchaq, ¿jrub' junab' k'mol panadería inwi'l lojori?    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> lo menos que,que taqal jun cuarenta y cinco o cuarenta y siete años win xij innegocio re panadería lojori y ajwichVR win xilj jowjari, ari' tkan'eeloq cuand inb'ek,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE> ijk'am b'i qaqaaj Dios, arruk' re' tkan'eel man negocio li.  B'ay k'omo chawechaq &lt;oja&gt; oj'ab'an taq tomar en cuenta, tanb'ij la verdad lo que nemo naq xink'ut chuch k'is inrab'iin, ink'ajol xk'iy taq,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE> lojori gracias a Dios juntiirVR, juntiir xmaj taq chaak, a la' chalónik inchaakin jilon xmaj taq rechaq también y k'omo </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a60" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> porque nen xya'w &lt;jsowr&gt; tra jpensaraq li laj b'aa'aq xmaj chaakaq qaqaaj Dios, porque re juntiir tjya' laj qapensar, jik'uno li wxtub'-wech'elxiik xaq li kiib'-uxub' ra lintziij	   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a61" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> tanb'ij chawechaq, k'omo oj'ab'an taq tomar en cuenta, arite Dios tya'w achoq'aab'aq y ret Dios ojchajin ák'laq, el únicamente xaq li kiib'-uxub' intziij tanb'ij chawechaq. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Tomás Alberto Méndez López" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="M.D" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Buenos días paisanos-familiares les doy un cordial saludo en nombre de nuestro Dios, que amanecimos bien, que no nos pasó nada,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> que pasamos otra noche, gracias al gran Dios, les doy un cordial saludo, gracias; vamos a decir unas nuestras palabras de nuestro lugar, de nuestro pueblo.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> Antes cuando yo subí aquí, yo era, estaba mi casa por el calvario con mis padres con Matías Méndez, con Juana Damián,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> ahí estábamos nosotros en el calvario, ahí estaba la casa de mis padres y me fueron a traer por una humilde pobre persona, mi dueño, mi esposo que me fue a traer,     </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> hicimos nuestro casamiento, fue así que conocí Jacubí subí, cuando vine yo aquí el camino solo era una vereda, no había carretera, no había nada, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> pero poco a poco los humildes paisamos y familiares se les ocurrió construir una escuela, construir donde se les pudiera dar clases a los pobres niños,    </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> eso fue lo que hicieron. Después creció Jacubí, arreglaron su carretera porque en Jacubí antes había un gran río, "el río de Jacubí" </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> gran río y caminaba, pero como se secó ya no había agua, mas de algo se hizo y porque antes ese río con ganas se agitaba, antes era muy fuerte, era muy grande,    </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> pero como tiene su fin, se terminó, ya no hay, bueno gracias a ustedes que nos toman en cuenta para decir nuestras palabras sobre nuestro pueblo.    </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> Antes cuando yo crecí, ese mercado antes había mucha pobreza, había mercado solo en el convento,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> mucha pobreza en nuestras cocinas antes, nosotras eramos cocineras antes, yo aún era pequeña, per ya teníamos nuestra cocina con mi abuela Matea Cabrera,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> y ví salir el mercado del convento, fue el sacerdote padre Gerardo quien sacó ese mercado del convento,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE> lo pasó al nuevo mercado de ahora. De eso pueblo en general nos damos cuenta de cómo hizo nuestro pueblo antes, eso es,     </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> y vine yo aquí, ví también como arreglaron la carretera, como arreglaron la carretera, esa carretera es la que va a Chejam, no había carretera, solo una vereda es la que iba,    </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE> pero poco a poco se levantaron los albañiles, arreglaron la carretera que va para Chijam, creció, arreglaron un camino donde sube sobre el cerro de Jacubí</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE> que también se amplió, pero es eso lo necesario de que nos demos cuenta de todo, de como suciedió en nuestra comunidad,     </ANNOTATION_VALUE>
