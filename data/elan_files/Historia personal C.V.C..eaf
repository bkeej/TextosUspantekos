        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> Pues iin inch'a'w xib' jun ra, jun ra yolooj tinb'an lajori eh jwi'l man yajeel tjiin tye' rib' lajori laq qak'aslemal neri Tz'unun Kaab',  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> tjiin tqil pues que lajori ojb'e na &lt;lo&gt; l k'áyb'l, ojb'e na chojloq'ik kúmb'l ltqatij che', pero iin, ooj re xink'iy rk'il inmamá, rk'il inchuuch, rk'il inqaaj,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> ¿nen jb'iij? qe ooj puro worron tijye' qamamá chqe, ¿nen jb'iij?, tjye' ¿jb'iij ne kúmb'lVR chqe? man ah</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> tjtok tjye' chqe, tran qabaño xilj, tjtzak, tiqatij y ojb'e l qasok, y tsaqárk, &lt;oj&gt; ojb'ítk, ya ojtzi chaq pues </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> y ri' chwaj li iin, ma ta' tqab'an, qak'ex qacostumbre rk'il qachuuch-qaqaaj, siempre tqatij kúmb'lVR wrron pues, porque ri mejor li chqe,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> ya kúmb'l tqaloq'e' chik re a veces tran k'ex chqe, toces lajori tinb'ijin qab'ne' seguir nen xij taq qatiit'-qamaam chqe ójrVR,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a38" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> porque we iin b'noli' seguir inwi'l tintij kúmb'l worron, tintok wi' ¿nen jb'iij?, re ojob' ttijsaj re&lt;ee&gt; </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a39" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> ¿nen jb'iij? om, berbena che' taq re, xe' laj &lt;in&gt;inb'aa nen jb'iij, eh tqatij re q'oxon b'aa, re q'oxon tyojiil,      </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a40" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> re cuando wi' q'aaq' chqij ri' li tqatij, toces ttijmaj qawi'l, ojb'e laj qasok, oj-eel l k'atan, y ya chi jkaab' q'iij ojtzi chaq. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a41" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>  Tonces ri' tqab'ne' seguir li tinb'ijin, y cuando wi' k'axk'ol &lt;jqapa&gt; qach'ool tqatij i'y, wta' tqatij i'y, tqatij, ¿nen jb'iij? man semilla tqatij,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a42" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE> también tqatijile', tqatij li toces, ri' li kúmb'lVR ttijsájk y tinb'ijin re wk'lal wunaq -tqatij, tije' kúmb'lVR worron- inche' re, para que  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a43" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> re ta' tran k'ex chqe pues, porque a veces tran k'ex re qach'ool o tye' chik jun yajeel chqe chik, toces ri' li tinb'ijin. Y ojor taq tziij, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a44" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> tinb'ij chik jun ra kiitz re intema, ¿nen k'iyb'al xinwa'x rk'il inqaaj-inchuuch? ójr, ójr ojb'e le chaak, ójr, ójr ojxanb'ar chirij inpapá, ojb'e l chaak,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE> tqatike' ab'iix, ojchaáakun rk'il asroon, ojcháakun rk'il mchet, cuando ojq'ajch b'esal q'iij, tqab'anch qási', ojpet chuch jaa,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a61" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE> ojyuq chuch jaa, tqab'ne' na &lt;qa&gt;qatarea nen jb'iij, &lt;qa,qa&gt;tqajche' &lt;la&gt; le escuela chi jkaab' q'iij; tonces ri' li tinb'ijin y lajori tinwil&lt;ll&gt; </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a62" TIME_SLOT_REF1="ts61" TIME_SLOT_REF2="ts62">
                <ANNOTATION_VALUE> wk'lal tinwil wunaq b'ik ri' y ¿nen jb'iij? ah ta' raj taq tran taq estudiar, ta' raj taq tjb'an taq ah lajori ta' chki' tcháakun taq,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a63" TIME_SLOT_REF1="ts63" TIME_SLOT_REF2="ts64">
                <ANNOTATION_VALUE> xaq jtarey, xaq jchaak chaq tran taq nen tye' taq re lescuela, toces lajori tinb'ijin eh ¿nen? saber ¿nemo vida? tk'iy taq lajori kristyan pues,     </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a64" TIME_SLOT_REF1="ts65" TIME_SLOT_REF2="ts66">
                <ANNOTATION_VALUE>pero qech ooj triste qavida xojk'iy rk'il inqaaj-inchuuch, xojk'iy aldea &lt;l&gt;las Marías Macalajaw, kla' xojk'iy ooj, xkam b'i inqaaj, xkam b'i inchuuch,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a65" TIME_SLOT_REF1="ts67" TIME_SLOT_REF2="ts68">
                <ANNOTATION_VALUE> pero qetami'n nenen xkaj k'ute' chqáwx, &lt;reta&gt; qetami'n qachaak, qetami'n juntirVR y ri' también ojyoloow laj qatziij ri' inmamá xij ri chqe ójrVR,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a70" TIME_SLOT_REF1="ts69" TIME_SLOT_REF2="ts70">
                <ANNOTATION_VALUE> mija yoloow taq wk'il laq qatziij porque rojóxkin tche', ttaw jun q'iij rojóxk chawechaq, chawaj taq ta'an taq ah, tatok taq achaak o nen tab'an taq, awetami'n donde quiera atb'ansajw tomar    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a71" TIME_SLOT_REF1="ts71" TIME_SLOT_REF2="ts72">
                <ANNOTATION_VALUE>  en cuenta cha' taq chqe y qamamá, pero ooj ta' xqab'an tomar en cuenta lo que xij chqe, porque inpapá tch'a'win  k'iche' y ojoj, inmamá tch'a'w &lt;r&gt; qatziijb'al Uspanteko, </ANNOTATION_VALUE>
