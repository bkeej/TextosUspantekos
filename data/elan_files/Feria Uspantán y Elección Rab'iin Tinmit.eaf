    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="E.M.T.L" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Las personas antepasadas hacian nuestra fiesta</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> para mayo, en el mes de mayo hacian nuestra fiesta,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> siempre cuando ya estaban apunto de hacerla, empezan las fuertes lluvias, entonces por eso lo trasladaron para el mes de septiembre,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> como en septiembre no había lluvia, entonces lo establecieron, trasladandolo para septiembre. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> Fue así como encontraron al Santo (patrono del pueblo) allá</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> en sacapulas (municipio de Quiché) así le dicían a aquel lugar y ahí lo fueron a encontrar (al santo) y como eran fuertes (las lluvias) </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> lo trajeron para aca (al santo)  así cuentan las personas antepasadas.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> Ellos lo trasladaron y como eran fuertes las lluvias que se inundaba todo, entonces ya no podían pasar las personas para ir a ver la fiesta, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> pero dejaron una elección para elegir, para escoger una señorita para que... </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> que fuese a exhibir nuestra vestimenta a otros pueblos, en el mes de mayo, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> en mayo se busca a una señorita, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> para ir a dar a conocer nuestra vestimenta, para que vaya a dar a conocer nuestro idioma a otros pueblos, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE> ahí, llegaban varias señoritas, elegían a la que deseaban que fuese a dar a conocer nuestra vestimenta;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> iban a dar a conocer ahí, pasaban, les daban oportunidad de hablar, hablar en nuestro idioma, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE> se podía dar a conocer ¿qué era lo que había realizado la senorita antes? y ¿qué hacen en ahora?.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE> Ahora nosotras la señoritas ya tenemos nuestra...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE> ya hay muchas oportunidades que se nos otorgan ahora, no como antes a nuestros abuelos no se les daba lugar a que hicieran cualquier cosa, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE> ahora si ya dejan, ya nosotros vemos que hacer, que ya podemos hacerlo ahora y </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE> nuestros abuelos ya están dejando en el olvido nuestro idioma, ya no les dicen nada a los niños, ya nada </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE> hablan con sus hijos, ni en nuestro pueblo, ni el mercado, ya no hablan, ¡ya no hablan nada!, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE> van por sus compras, ya no se están, ya nada dicen en nuestro idioma, ya no se ponen nada de nuestra vestimenta; lo que queremos nosotros</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE> es que usen nuestro vestuario, que lo usen, que hablen en nuestro idioma ¿con quienes? con los niños, con las señoritas, con los jovenes,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE> ahí donde viven; queremos que no se pierda nuestro idioma y así que aún podamos ser bastantes (uspantekos) en nuestro pueblo y</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE> ya que se ve bien que un pueblo use su vestimenta, que hablen en nuestro idioma, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> y así podamos crecer como pueblo (Uspanteko)... </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE> Gracias a ustedes por escucharme, yo me llamo Esmeralda Micaela Túm López, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE> estoy aquí en mi pueblo San Miguel Uspantán que también le llamamos Gorrión Dulce. Muchas gracias a ustedes. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
