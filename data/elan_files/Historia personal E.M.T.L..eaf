    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="E.M.T.L" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Buen día a todos hasta donde me escuchan, ahora les relato un poco a ustedes, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> de como gané para ser Hija de nuestro pueblo de Uspantán (Reina Uspanteka).</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> Ahora les cuento a ustedes que yo participe en eso, sin saber donde, a dónde tenía que ir y qué tenía que hacer, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> llegué ahí, hablé en nuestro idioma, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> no sabía, si yo iba a ganar, ahí gané y me permitieron que fuera,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a64" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> que fuera a otros pueblos, para ir a mostrar nuestro idioma, y fui a mostrar nuestra vestimenta a otro pueblos.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a65" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE> Ahí fui a hacer, fui a conocer a las señoritas, que eran de allá de Quetzaltenago, ( Departamento de Guatemala) </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a66" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> ahí fuimos con la flor, flor de ese pueblo (Reina del Pueblo Quetzalteco)</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a67" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> fuimos, platicamos sobre nuestra vestimenta, sobre ¿cuál es el trabajo de las señoritas ahora?;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a68" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> el trabajo de las señoritas ahora, es hacer, es ir a mostrar sus vestimentas, ir a mostrar </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a69" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>  lo que hacen en sus pueblos, que lo puedan ir a relatar.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a70" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> Ellas hablaron sobre, los hombres que no hacen, que no les dan </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE> las cosas a ellas y ellas quieren salir de su </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> de su sufrimiento,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE> ahora con ellas ahora fuimos a hacer (participar) y eligieron a una, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE> y ahora junto ellas estuvo bien, que hayamos ido a ver y a conocer a otras personas, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE> de ver ¿cómo son nuestras vestimentas?, ¿de dónde venimos?, ellas </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE> nos dieron una nuestra... una nuestra...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a35">
                <ANNOTATION_VALUE> así como les decía sobre las señoritas.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a36">
                <ANNOTATION_VALUE> Y ahora aquí en nuestro Uspantán, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a37">
                <ANNOTATION_VALUE>  ¿qué hacemos nosotros?  nosotros hacemos, nuestros abuelos hacen, hacen ollas de barro, hacen comales,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a38">
                <ANNOTATION_VALUE> y nuestro corte es negro, y nuestro güipil es blanco, con cuello rojo, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a39">
                <ANNOTATION_VALUE> nuestro collar le dicen que significa las montañas, nuestro corte negro es por la oscuridad </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a82" ANNOTATION_REF="a40">
                <ANNOTATION_VALUE> de la noche, así como le decimos y nuestra cinta (se pone en el cabello) es;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a83" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE> nuestra cinta, es roja, es roja y</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a84" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE> esa es nuestra cinta que llevamos nosotras la mujeres, nosotras las mayas, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a85" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE> de Uspantán. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a86" ANNOTATION_REF="a44">
                <ANNOTATION_VALUE> Yo me llamo Esmerlda Micaela Túm López, aquí estoy en Uspantán. Muchas gracias a ustedes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
