        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a843" ANNOTATION_REF="a372">
                <ANNOTATION_VALUE>Ya titaw hor atxjoow taq, ya taw hor re nimq'iij. " "¿Ri li tamay? ¡iin atint'o'w!" "Per ¿nin inat'o'w re?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a844" ANNOTATION_REF="a374">
                <ANNOTATION_VALUE> Ta' tab'ij y atint'o'w." "Katnatun b'i jli </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a845" ANNOTATION_REF="a376">
                <ANNOTATION_VALUE> y tinb'ij chawe nen tinya' chawe."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a846" ANNOTATION_REF="a378">
                <ANNOTATION_VALUE> "Tzib' kali," tche. Xnatunch &lt;ra&gt; ra jxúkt eh tinmit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a847" ANNOTATION_REF="a380">
                <ANNOTATION_VALUE>Y cuand xq'aj chrij, tb'ij re, "¿Ri li itz'yaq chawaj?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a848" ANNOTATION_REF="a382">
                <ANNOTATION_VALUE> ¿Ri li lo que tamay?  Cha' jun awe  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a849" ANNOTATION_REF="a384">
                <ANNOTATION_VALUE> y tak'am b'ik per ni jumul tab'ij re kristyan lamas xame'w."  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a850" ANNOTATION_REF="a386">
                <ANNOTATION_VALUE> Pax ránm tril ritz'yaq ajxójol, tqopqon, tqopqon  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a851" ANNOTATION_REF="a388">
                <ANNOTATION_VALUE>li puwaq. Y tijb'ij , "¿Nin k'uli xinb'an ri,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a852" ANNOTATION_REF="a390">
                <ANNOTATION_VALUE>ri li xyuqinte' y ri li tink'am b'ik?"</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a853" ANNOTATION_REF="a392">
                <ANNOTATION_VALUE>Va cha' jun awe y tak'am b'ik y ri takoj laj mer jq'íjl qatinmit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a854" ANNOTATION_REF="a394">
                <ANNOTATION_VALUE> Per tinb'ij b'i chawe ta' tab'ij re ni jun, que niri xyuqame'w.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a855" ANNOTATION_REF="a396">
                <ANNOTATION_VALUE> "Jaan," xche'. Xk'amch atz'yaq, tki'kot ránm. Ta' chki xk'amch jsii'. Ta' chki xk'amch ni'j qleen.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a856" ANNOTATION_REF="a398">
                <ANNOTATION_VALUE>Reqajch atz'yaq, xk'amch chirchooch, y xyuq chirchooch ta' xij re rixóql, ni rech'elxiik, ni jun.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a857" ANNOTATION_REF="a400">
                <ANNOTATION_VALUE>Y jpach ti-'el taq li xjooj xij taq re, "¿Y akúchj? ¿Nin tab'an re?" "No, iin ta' tinye' inkúchj."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a858" ANNOTATION_REF="a402">
                <ANNOTATION_VALUE>"Per ¿nin tab'an re awitz'yaq?""Pues nin tinb'an tre. Tinwilb'ej na taw q'iij y rik'u re' tinjutun &lt;cha&gt; chaxo'laq."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a859" ANNOTATION_REF="a404">
                <ANNOTATION_VALUE>"Jaan," xche' taq re, "Per ¿nin k'u tran wunaq li? Ta' ta' xya' jkúchj re ritz'yaq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a860" ANNOTATION_REF="a406">
                <ANNOTATION_VALUE>Y ¿nin tqab'an re? Wa'x naq kla', tril li jcholájl."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a861" ANNOTATION_REF="a408">
                <ANNOTATION_VALUE>Xtaw jq'íjl eh roox q'iij re jnimq'iij eh Simyel.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a862" ANNOTATION_REF="a410">
                <ANNOTATION_VALUE>Xjoow taq y xkojch ritz'yaq wunaq, ¡Tqopqon! ¡Tqopqon! &lt;li li li&gt;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a863" ANNOTATION_REF="a412">
                <ANNOTATION_VALUE>li q'aaq' ritz'yaq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a864" ANNOTATION_REF="a414">
                <ANNOTATION_VALUE>Y xril taq juntir. "¿Lamas xame'w awitz'yaq? Ta' &lt;ke'&gt; ke' apuwaq, ta' &lt;ta' ta'&gt; ameer, ta' akortiil.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a865" ANNOTATION_REF="a416">
                <ANNOTATION_VALUE>Atb'eyom ta'n,y nin k'u &lt;ma&gt;man atz'yaq li ¿li xame'w?"  Ta' tjb'ij.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a866" ANNOTATION_REF="a418">
                <ANNOTATION_VALUE>Y ta' tijib'ij, tjk'ot taq jchii', "¿Per lamas xame'w awitz'yaq?"</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a867" ANNOTATION_REF="a420">
                <ANNOTATION_VALUE>Ta' tijib'ij, va, taxan jun wi' jun chijxo'laq te.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a868" ANNOTATION_REF="a422">
                <ANNOTATION_VALUE> "Qaye' awryent re, qaq'ab'rsaaj.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a869" ANNOTATION_REF="a424">
                <ANNOTATION_VALUE> Ya sache'l chaq jb'aa, tijyola' li chqe lamas xijme'w."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a870" ANNOTATION_REF="a426">
                <ANNOTATION_VALUE>"Jaan," xche' taq, &lt;x,x&gt; xkoj, xya' taq kúmb'l re.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a871" ANNOTATION_REF="a428">
                <ANNOTATION_VALUE>xq'ab'rsaaj taq, ya laj q'ab'árk, xelch laj chii'.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a872" ANNOTATION_REF="a430">
                <ANNOTATION_VALUE> Tijb'ij rechaq, "Pues lamastch xinme' witz'yaq, jeli b'a loom."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a873" ANNOTATION_REF="a432">
                <ANNOTATION_VALUE>"¿Lamas loom?" "Jeli b'a Xoqoneb'".</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a874" ANNOTATION_REF="a434">
                <ANNOTATION_VALUE>"¿Nin k'u xab'an?" Xelch jun nimlaj wunaq chwiij.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a875" ANNOTATION_REF="a436">
                <ANNOTATION_VALUE>Y xij chwe que, "¿Nen chak inb'esónk?" Cuando wi' la jq'aab' re re</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a876" ANNOTATION_REF="a438">
                <ANNOTATION_VALUE> utziil re jt'o'w y kla' xinme'ch wítz'yaq."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a877" ANNOTATION_REF="a440">
                <ANNOTATION_VALUE> "¡K'uttaj chqáwch!" "Loq'ori ta' chki inkwiin, ta' chki tinte'."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a878" ANNOTATION_REF="a442">
                <ANNOTATION_VALUE>Y kla' k'uli &lt;tiji&gt; tyol taq qamaam qatiit'. Que kila' wi' jun meer jkortii'l</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a879" ANNOTATION_REF="a444">
                <ANNOTATION_VALUE> qatinmit Tz'unun Kaab'. ¿Nin k'u chak &lt;x&gt;xq'aje b'ik?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a880" ANNOTATION_REF="a446">
                <ANNOTATION_VALUE> ¿Nin k'u chak xsach b'ik? Mat xijyol na che'.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a881" ANNOTATION_REF="a448">
                <ANNOTATION_VALUE>Mat xel na laj chii' wunaq, xijk'ol taj.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a882" ANNOTATION_REF="a450">
                <ANNOTATION_VALUE>Xijch'uqt jb'aa lo que xi'jsaj re, jpuwaq qatinmit Tz'unun Kaab', ajwi' tna re.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a883" ANNOTATION_REF="a452">
                <ANNOTATION_VALUE>Cuand xretamaj wunaq, retamaj nimalaj wunaq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a884" ANNOTATION_REF="a454">
                <ANNOTATION_VALUE>Xyolsaj jmeb'iil qatinmit,  q'iij re xijyol.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a885" ANNOTATION_REF="a456">
                <ANNOTATION_VALUE> Xtormaj jun lel B'a Kameb', xnatun b'ik </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a886" ANNOTATION_REF="a458">
                <ANNOTATION_VALUE>Kób'n. Xnatun b'ik Rab'inal, kla'.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a887" ANNOTATION_REF="a460">
                <ANNOTATION_VALUE>Xtok b'i jb'ee jmeer jkortii'l.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a888" ANNOTATION_REF="a462">
                <ANNOTATION_VALUE> J'upuwaq Tz'unun Kaab'.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a889" ANNOTATION_REF="a464">
                <ANNOTATION_VALUE> K'ixk'olil eh xtamaj ójr taq tziij. Ójr taq qamaam qatiit' tib'eson taq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a890" ANNOTATION_REF="a466">
                <ANNOTATION_VALUE>Tajb'ij taq, kila' tawem ójr, nimalaj k'achelaj.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a891" ANNOTATION_REF="a468">
                <ANNOTATION_VALUE>Ta' qas ttaw jun kla' porque k'a'ni'n, k'a'ni'n loq'laj mundo kla'.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a892" ANNOTATION_REF="a470">
                <ANNOTATION_VALUE>Ri li xkan yolsaj jwi'l qaqaaj José Méndez.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a893" ANNOTATION_REF="a472">
                <ANNOTATION_VALUE> Ri li qamaam qatiit' xkan yoloow taq y xi'j taq, "Xqak'ol tine'.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a894" ANNOTATION_REF="a474">
                <ANNOTATION_VALUE>Xqachajaj tine' &lt;jloqo&gt; jloq'óxl y &lt;jloq, loq,loq&gt; loq'b'al puwaq re qatinmit Tz'unun Kaab'."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a895" ANNOTATION_REF="a476">
                <ANNOTATION_VALUE>Ri li tinyol jun saqrab'em ri rej eh...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a896" ANNOTATION_REF="a478">
                <ANNOTATION_VALUE> Rik'al junab' y tink'amwaj chawe ataq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Robert Henderson" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción normativa" PARTICIPANT="Virgilio Tomás Méndez" TIER_ID="Morfología">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a897" ANNOTATION_REF="a322">
                <ANNOTATION_VALUE>T-Ø-in-k’amwaj              re       jun saqrab’eb’ ri. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a898" ANNOTATION_REF="a324">
                <ANNOTATION_VALUE>Ch-Ø-w-aj                t-Ø-in-ye’               j-b’íj-ik         nen  j-b’an-taj        Tz’únun Kaab’. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a899" ANNOTATION_REF="a326">
                <ANNOTATION_VALUE>X-Ø-Ø-kaj               yol-e’     taq  qa-maam,   qa-tiit’.    Ójer                 taq   kristyan neri,  x-Ø-ij-yol                taq.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a900" ANNOTATION_REF="a328">
                <ANNOTATION_VALUE>Nen chak k’uli? Nen  x-Ø-tamaj                b’a    Xoqoneb’?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a901" ANNOTATION_REF="a330">
                <ANNOTATION_VALUE>Ti-Ø-j-yol                taq   qa-maam,    qa-tiit’        wi’   jun  qub’   aj-xoj-ól-ob’,                     t-Ø-’el           taq   li     nimq’iij  qa-tinmit.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a902" ANNOTATION_REF="a332">
                <ANNOTATION_VALUE>Y taw    nojel ab’,    t-Ø-ij    -b’ij       taq,    “t-  Ø-  qa-        tok            qa-pach,                   t-     Ø-qa-mol               q-ib’,          t-Ø-q-esaj                xójol.”</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a903" ANNOTATION_REF="a334">
                <ANNOTATION_VALUE>X-Ø-taw             taq   chuuch. X-Ø            -tok     taq   j     -pach         -aq.   X-Ø-        nuk’   j    -yolj  -aq. Y wi’  juun</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a904" ANNOTATION_REF="a336">
                <ANNOTATION_VALUE>x-Ø-Ø-ij,               “In-el        a-k’l-aq."        X-Ø-taw            q’iij t-Ø-ij-met                t-Ø-ij-me                taq-ch    r-itz’yaq  aj-xójl </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a905" ANNOTATION_REF="a338">
                <ANNOTATION_VALUE>li      K’íche’  o r-ik’il Aj Máx-ib’.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a906" ANNOTATION_REF="a340">
                <ANNOTATION_VALUE>Y wi’   jun  rechaq x-Ø-u-may              j-wiich,  “Nin  t-Ø-in-b’an?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a907" ANNOTATION_REF="a342">
                <ANNOTATION_VALUE>Ta’    in-puwaq. Ya ti-Ø-taw            q’iij oj-’el         li     xójol.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a908" ANNOTATION_REF="a344">
                <ANNOTATION_VALUE>Y ¿nin   t-Ø-in-b’an              ri?” Ti-Ø-b’ison             r-ánim. Ta’    t-Ø-Ø-ta’-w                       nen  t-Ø-r-an.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a909" ANNOTATION_REF="a346">
                <ANNOTATION_VALUE>X-Ø-k’am         b’i   r-íkaj. X-Ø-k’am        b’i    j-kólob’. X-Ø-k’am         b’i   j-pátan.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a910" ANNOTATION_REF="a348">
                <ANNOTATION_VALUE>X-Ø-e’          b’a  Xoqoneb’, t-Ø-b’ison-ik. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a911" ANNOTATION_REF="a350">
                <ANNOTATION_VALUE>“Ta’    nentaj   nab’ej,   ri       t-Ø-r-il,              x-el-ch               jun  nim-laj         wunaq </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a912" ANNOTATION_REF="a352">
                <ANNOTATION_VALUE>b’a   k’achelaj  y x-Ø-ij                re,   “¿Nen    t-Ø-a-b’an               neri?”</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a913" ANNOTATION_REF="a354">
                <ANNOTATION_VALUE>“Sii’  t-Ø-in-tok.” “¿Per   nen  t-Ø-a-may?” "Ta’    nen  t-Ø-in-may."</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a914" ANNOTATION_REF="a356">
                <ANNOTATION_VALUE>“Per   iin t-Ø-inw-il            que wi’   jun  b’is       laj    aw-ánm. B’ij   ch-w-e,       at-in-t’o’-w.”</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a915" ANNOTATION_REF="a358">
                <ANNOTATION_VALUE>“Per   nin   in-a-t’o’-w                si  aat, ta’      aw-etam  lo que wi’ laj    w-ánim,          w-etam-i’n.”</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a916" ANNOTATION_REF="a360">
                <ANNOTATION_VALUE>“Si we      t-Ø-a-b’ij               ch-w-e,  at-in-t’o’-w.”</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a917" ANNOTATION_REF="a362">
                <ANNOTATION_VALUE>T-Ø-b’ison              r-ánim,         ta’      raj     t-Ø-b’ij re.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a918" ANNOTATION_REF="a364">
                <ANNOTATION_VALUE>“Per   iin ch-Ø-w-aj             in-a-t’o’-w,                per   aat   ta’      at-kwiin-ik.” “¡In-kwiin-i’n!       B’ij   nika    ch-w-e            nin   b’is       wi’    laj    aw-ánm.” </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a919" ANNOTATION_REF="a366">
                <ANNOTATION_VALUE>“Ya ti-Ø-taw           j-q’iij  qa-tinmit    y wi’-n       li    xojooj.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
