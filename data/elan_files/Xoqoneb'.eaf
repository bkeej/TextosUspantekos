        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a380" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE>Y ri' xq'ax chirij, tijb'ij re, "¿ri' li itz'iiq chawaj?</ANNOTATION_VALUE>
