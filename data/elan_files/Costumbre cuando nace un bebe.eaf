    <TIER ANNOTATOR="Tomás Alberto Méndez López" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="M.A.V.C" TIER_ID="Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a71" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE> Buen día señor Tomás, ahora voy a decir un poco de lo que entiendo sobre este papel.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a72" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE> Ya hace una hora que vino mi familiar, que vino mi familiar,entró a la casa, lo vi con mis hijos.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a73" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE> Todos los días la señora va al mercado en la mañana. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a74" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> Viene al pueblo de Uspantán, pasa a la par del campo donde se hacen juegos.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a75" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> Se bañó en la mañana un hombre. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a76" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE> Se baño en agua caliente ahorita.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a77" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE> Todos los días por la mañana es necesario bañarnos. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE> Salió un gato de la tienda.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE> Salió agua. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE> Salimos a Cobán en la mañana hace ratos.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a81" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE> Salí de mi casa hace ratos en la mañana.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a82" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE> Llora la señora cuando llegó al cementerio.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a83" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> Llora el niño porque se cayó. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a84" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE> Entramos a ver las películas. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a85" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE> Vivió un bebé. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a86" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE> Vivió un/una . . .</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a87" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE> Yo viví en Uspantán. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a88" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE> Voy a contar un poco sobre el modo en que nace un bebé, antes nuestros abuelos eh nos contaban a nosotros de que modo,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a89" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE> como le hacen nuestros familiares cuando nace un bebé, ellos agradecen ante nuestro padre Dios, que llega otro niño a la faz de la tierra,   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a90" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE> es por eso que hacen una fiesta, hace ah, se reunen en la casa de la mujer que dió a luz en bebé. Nos cuenta </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a91" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE> mi mamá a nosotros que antes ellos lo hicieron, lo hacían en grande, se reunían, comían</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a92" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE> eh las personas, cuantos llegaran con ellos; nos decía ella que se usaba   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a93" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE> eh carne de gallina nos decía, había una costumbre que se hacía, se mataban gallinas, las tortillas que se comían eran tamalitos siete camisas dice,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a94" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> había quienes lo usaban así, habían otros que usaban carne de vaca y tamalitos, habían otros que usaban carne de oveja decía, eso se utilizaba antes,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a95" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE> no se hacía allá como se hace ahora, ya diferente lo hacen las personas, ya no quieren recibir nuestra costumbre, se avergüenzan de hacerlo, ya no quieren enseñar como es nuestra cultura aquí en la tierra, de que modo es nuestra cultura nosotros los uspantekos.   </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a96" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE> Eso es lo que les di un poco a conocer a ustedes, cuantos lo escucharon, esto es lo que sé sobre nuestra cultura como uspantekos. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a97" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE> Ah yo, yo soy maestra, trabajo allá en la comunidad de Macalajau, trabajo con los niños, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a98" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE> los pequeñitos de la escuela, ahí eh se ve bien, las personas ahí trabajan y dan . . . </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a99" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE> cuidan la naturaleza, ahí aún hay árboles, aún hay siembra, no es igual como aquí, a como estamos aquí en nuestro pueblo,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a100" ANNOTATION_REF="a31">
                <ANNOTATION_VALUE> ya no hay, es por eso que hay eh muchas enfermedades que estamos padeciendo, nosotros mismos se lo estamos haciendo a la naturaleza, no queremos valorarlo,     </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a101" ANNOTATION_REF="a32">
                <ANNOTATION_VALUE> ahora estamos viendo todas las enfermedads que se están escuchando aquí en la faz de la tierra y es por eso que les digo a las personas: -hagan un favor,  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a102" ANNOTATION_REF="a33">
                <ANNOTATION_VALUE> ayúdemonos  a cuidar la naturaleza, solo así estaremos felices sobre la sagrada tierra.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a103" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>  Les agradezco a ustedes, cuanto pude contarles a ustedes, gracias por escucharme, será nuestro padre Dios quien se quede con ustedes, agradezco ante ustedes.  </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
