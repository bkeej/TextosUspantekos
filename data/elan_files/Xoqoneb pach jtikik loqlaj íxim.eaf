    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="annotation" PARENT_REF="Transcripción" PARTICIPANT="J.E.P.U" TIER_ID="Descripción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a151" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE> Nimq'iij Simyel, se refiere a la feria patronal de Uspantán en honor a San Miguel Arcángel. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
