    <TIER ANNOTATOR="Tomás Alberto Méndez" LINGUISTIC_TYPE_REF="default-lt" PARTICIPANT="M.V.A" TIER_ID="Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>  Saqari'n Qaaj, saqari'n Qaaj, saqari'n Looy, k'omotzáwch, k'omotzáwchaq, k'omo re Qaqaaj Dios wat taq qk'il neri la jaa, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE> qak'omowaj re qaqaaj Dios, que re qaqaaj Dios xk'omówch ab'ee'aq neri, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE> porque re qaqaaj Dios wi' nab'ee chqáwch y ruk' re wi' woj ooj. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE> Iin tanyol jun kiitz chawechaq lo que xqab'an ooj, cuand xojk'iych rik'il inqaaj, inchuuch ójr; </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>cuando ooj núuch', cuando ttaw KuréxmVR </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE> &lt;tuqa&gt; tqakoj qatzii, re &lt;tqa&gt; qatuk qamaatz re kuréxm, tqab'an &lt;qa&gt;qanimq'iij kuréxm, kom jor tki'kot qánm</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE> porque titaw kuréxmVR, tqab'an qanimq'iij, inqaaj-inchuuch &lt;tran&gt; tran taq qakaxlanwaa, tijtuk taq qakaab' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE> ri li tqatij aq'aab' y tqatij &lt;qa&gt; tqatij quk'a' xilj tra qamuul, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE> ri li tqatij aq'aab', b'ay, re &lt;qab',qa&gt; kab'lájk</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE> &lt;tqa&gt; qatzak qakinaq', saq kinaq', y tqajo'k' ra rij; </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE> tqakye'j ra qak'oxb'l b'a kaa' y tqatzaq ra kiitz q'oor rik'il y ra kiib'-uxiib' chqiij iik' y tqajo'k' b'a kaa', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>  y ri li tqatzaq chirij &lt;ra qa&gt; ra qasaq kinaq' re qakab'lájk y tqaleej qasaqmólb'VR </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE> tqaleej l xoltiin y tri li tqatij re qakab'lájk y tqaleej qaq'oor y tqasub'aj ni'j </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE> y tqatzak l b'o'j.  Ri li rtqab'an ójr, jor ojki'kot &lt;qa&gt; tki'kot qánm re ooj ójr cuando xojk'iy rik'i inqaaj-inchuuch </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE> porque xaq cuand ttaw kuréxm re', tqatij jun ra qasaqmólb', enter ra juun tya'saj chqe; porque ójr com más k'i qeb'il xojk'iy, entonces tb'ansaj</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE> inchuuch tran jun qasaqmólb' l leej y &lt;traj  pa&gt; tran paj ni'j re, ri li tjya' chqe ooj chqajujunal, tjach chqáwch  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a31" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE> per cuand ttaw kuréxm re' jor ojki'kótk, porque entos ruk' re tqatij jun ra qasaqmólb', enter ra jun tya'saj chqe,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE> &lt;y&gt; y jor tiki'kot qánm ttaw nimq'iij porque inqaaj-inchuuch ttzaq taq xaq chaj la jaa, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> y juntir ooj,  ooj anm ojsepar laq uleew, b'a xaq chaj y xaq wunaq tkub'ar taq b'a teem y qe ooj juntir ojsepar laq uleew. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a34" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE> Ri lajori com juntir xk'exmájk, lajori tqakoj tqakoj chaq teem ojkub'ar chaq ooj b'a teem, ójr ooj anm no, ooj juntir ojsepar laq uleew y laq uleew tqatij wi qáaw,      </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a35" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> ji', laq uleew tya'saj &gt;ra&gt; ra qarikiil xijl &lt;ra&gt; ra qálaq y ojsepar laq uleew tqatij qáaw, tya'saj ra qachikaach chqáwch  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE> jwi'l innaan, rik'i qáaw; tqak'am ra qáaw l chikaach, tqatij ra qáaw y jinoli jtijik qáaw tqab'an ójr. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a45" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE> y qe ooj neri la jaa juun qe inxib'aal wi', xaq inxib'aal li wi' ri re jb'iij Abelino, xaq qe li inxib'aal wi', ooj ooj seis anm xojwa'xn  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a46" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE> ooj seis anm; nab'ee, nab'ee watz ri xkami'n; re ta' chki' qk'il y xaq chaq xojkan ooj joob' anm xojkan neri la jaa rik'i inaan </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a47" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>Per ya juntir wich'elxiik ya rechaq ya juntir &lt;xxx&gt; xi' tqan,  xaq iin wk'an xinkan la jaa rik'il innaan,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a77" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>eh iin xqajách qib ooj pach</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a48" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE> porque qe ooj &lt;xqa&gt; xqajách qib' ooj &lt;pach&gt; pach jqaaj maq wik'laal, entos iin xinkan wk'anVR rik'i yaq wik'laal, iin kiib' wik'laal wunaq xwa'xk,    </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a49" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE> y xinkan wk'an rik'ilaq, iin xink'iysan &lt;rech, rk'i&gt; rechaq, xinchaakúnk y xink'iysaj wik'laal y ya xrijob' tqan</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a50" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE>  ya lajori kow taq chaq, wi' jun neri wik'il y &lt;juju&gt; jun b'esal jli nojo ák'laq jeli,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a51" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE> &gt;y&gt; y entonces ri li qe ooj woj la jaa rik'i innaan, joob' qeb'il   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a52" TIME_SLOT_REF1="ts61" TIME_SLOT_REF2="ts62">
                <ANNOTATION_VALUE> wi' innaan, &lt;won&gt; winVR iin, wi' wik'laal José, wi' wálb' Lucía y wi' ra wi'im ra núuch, ri li woj neri la jaa rik'i innaan, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a53" TIME_SLOT_REF1="ts63" TIME_SLOT_REF2="ts64">
                <ANNOTATION_VALUE> &lt;yyy&gt; y jinoli qab'anik, gracias a Dios tzii qab'anik rik'i innaan, woj neri la jaa, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a54" TIME_SLOT_REF1="ts65" TIME_SLOT_REF2="ts66">
                <ANNOTATION_VALUE> y ri innaan más k'ax wi'w, innaan k'ax chaq ránm, íwr xojpe Sant Cruz, &lt;xiqil&gt; xojpe rik'i jun ajkuun, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a55" TIME_SLOT_REF1="ts67" TIME_SLOT_REF2="ts68">
                <ANNOTATION_VALUE> re &lt;xil&gt; xrilch ránm &lt;nen&gt; ¿nen wi' re?, ¿nen k'ax re? y man ajkuun xijch chqe que ri ránm más xsipójk y wi' tra kiib' ib'och' xt'uqmaj cririj ránm. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a56" TIME_SLOT_REF1="ts69" TIME_SLOT_REF2="ts70">
                <ANNOTATION_VALUE> Innaan más chaq k'ex wi'w, lajori ojmeyow re, porque saber nen qaqaaj Dios xaq qaqaaj Dios li wi' jq'aab' chib', ¿nen tran re? porque re más chaq k'ex wi'w,      </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a57" TIME_SLOT_REF1="ts71" TIME_SLOT_REF2="ts72">
                <ANNOTATION_VALUE> más chaq k'ex wi'w, enter jun íwr xojq'aj chirij jeli Sant Cruz,  xyuqqej laj sok y xtz'ee'r enter b'enaq q'iij,  </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a58" TIME_SLOT_REF1="ts73" TIME_SLOT_REF2="ts74">
                <ANNOTATION_VALUE> &lt;yyy&gt; y &lt;ta'&gt; ta' chki, ta' chki'  tzii innaan, más chaq k'ex wi'w, porque ya re nim kristyan chaq.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a59" TIME_SLOT_REF1="ts75" TIME_SLOT_REF2="ts76">
                <ANNOTATION_VALUE> Ji'n, &lt;ri li&gt; ri li &lt;qa, rrr&gt; qak'axk'oliil woj lajori,  porque ya innaan &lt;más chaq&gt; más chaq, más chaq oom, más chaq k'ex wi'w,  puers chaq tran tb'itk,   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a60" TIME_SLOT_REF1="ts77" TIME_SLOT_REF2="ts78">
